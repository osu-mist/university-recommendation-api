Post
curl https://localhost:8080/api/v0/batch-scores \
    --insecure \
    --key doej.cer \
    --user "username:password" \
    --header "Content-Type: Application/json" \
    --data '{"id": null, "studentPool": { "province": "福建", "studentType": "文科", "batch": 2}, "year": 2015, "minScore": 400}'

